	<animation>
  		<type>select</type>
  		<object-name>Rotor-Pales-Queue</object-name>
  			<condition>
   				<less-than>
    			<property>/rotors/tail/rpm</property>
    			<value>1600</value>
   				</less-than>
  			</condition>
 	</animation>






	<animation>
		<condition>
			<greater-than>
				<property>rotors/tail/rpm</property>
				<value>1100</value>
